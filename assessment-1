const validator = function (str) {
  const match = {
    "{": "}",
    "[": "]",
    "(": ")",
  };
  const stack = [];
  for (let char of str) {
    if (match[char]) stack.push(char);
    else if (Object.values(match).includes(char)) {
      if (stack.length === 0) return false;

      const cl = stack.pop();
      if (match[cl] !== char) return false;
    }
  }
  return stack.length === 0;
};
console.log(validator("{ [ ] }"));
